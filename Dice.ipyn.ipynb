    "import random\n",
    "import tkinter \n",
    "import tkinter.messagebox\n",
    "\n",
    "top = tkinter.Tk()\n",
    "def dice():\n",
    "  #for i in range(0,1):\n",
    "  num = random.choice(range(1,6))\n",
    "  tkinter.messagebox.showinfo(\"Dice\",num)\n",
    "\n",
    "b = tkinter.Button(top, text=\"Dice\", command = dice)\n",
    "\n",
    "b.pack()\n",
    "top.mainloop()"
   ]
  }
